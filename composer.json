{
    "name": "chatagency/crud-assistant-blade-components",
    "description": "Blade components for the crud-assistant-extras package",
    "keywords": [
        "chatagency",
        "crud-assistant-blade-components"
    ],
    "homepage": "https://github.com/chatagency/crud-assistant-blade-components",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Victor SÃ¡nchez",
            "email": "victor@chatagency.com",
            "role": "Developer"
        }
    ],
    "repositories": [
      {
          "type": "path",
          "url": "../crud-assistant",
          "options": {
              "symlink": true
          }
      },
      {
        "type": "path",
        "url": "../crud-assistant-extras",
        "options": {
            "symlink": true
        }
    }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "chatagency/crud-assistant": "@dev",
        "chatagency/crud-assistant-extras": "@dev"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Chatagency\\CrudAssistantBladeComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chatagency\\CrudAssistantBladeComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chatagency\\CrudAssistantBladeComponents\\CrudAssistantBladeComponentsServiceProvider"
            ],
            "aliases": {
                "CrudAssistantBladeComponents": "Chatagency\\CrudAssistantBladeComponents\\CrudAssistantBladeComponentsFacade"
            }
        }
    }
}
